  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.0.4",
    "cross-env": "^7.0.2",
    "dayjs": "^1.9.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.0",
    "sharp": "^0.26.3",
    "tailwindcss": "^2.0.1"
  }
}
