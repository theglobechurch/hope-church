{
  "name": "hope-church",
  "version": "1.0.0",
  "description": "==================================================================================================",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel css eleventy",
    "eleventy": "eleventy --serve",
    "debug": "set DEBUG=* & eleventy",
